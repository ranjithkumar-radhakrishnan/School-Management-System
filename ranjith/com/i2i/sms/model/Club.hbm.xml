<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
          "-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
          "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">  
  
   <hibernate-mapping>  

          <class name="com.i2i.sms.model.Club" table="club">  
          <id name="id" column = "club_id">  
          <generator class="increment"></generator>  
          </id>  

          <property name="name" column = "club_name"></property>  
          <property name="president" column = "club_president"></property>  
          <property name="website" column = "club_website"></property>  
          <property name="count" column = "club_count"></property> 
          
          <set name = "students"  table = "student_club"  inverse="true" lazy="true" fetch="select">
          <key column = "club_id"></key>
          <many-to-many class = "com.i2i.sms.model.Student" column = "student_id"></many-to-many>
          </set>

          </class>       
            
   </hibernate-mapping>  