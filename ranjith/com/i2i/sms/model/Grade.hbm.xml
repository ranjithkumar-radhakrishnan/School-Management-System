<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
          "-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
          "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">  
  
          <hibernate-mapping>  
          <class name="com.i2i.sms.model.Grade" table="grade">  
          <id name="gradeId" column = "grade_id">  
          <generator class="increment"></generator>  
          </id>  
          <property name="standard"></property>
          <property name="section"></property>   
          <property name="sectionCount"></property>  
            
          <set name="students" cascade="save-update" inverse="true" lazy="true" >  
          <key>
            <column name="grade_id" />
          </key>   
          <one-to-many class="com.i2i.sms.model.Student"/>  
          </set>  
            
          </class>  
                        
          </hibernate-mapping> 